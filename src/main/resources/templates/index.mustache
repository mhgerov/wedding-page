<section class="probootstrap-section probootstrap-border-top" data-section="rsvp">
    <div class="container">
        <div class="row mb40">
            <div class="col-md-12 text-center">
                <h2 class="mt0 mb10 probootstrap-animate">Will you attend?</h2>
                <p>Please sign your RSVP</p>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                <div class="probootstrap-card probootstrap-animate">
                    <form class="probootstrap-form">
                        <div class="form-group">
                            <label for="name">Your Name</label>
                            <input type="text" class="form-control" id="name">
                        </div>
                        <div class="form-group">
                            <label for="email">Your Email</label>
                            <input type="text" class="form-control" id="email">
                        </div>
                        <div class="form-group">
                            <p>Will you attend?</p>
                            <label for="yes"><input type="checkbox" id="yes"> Yes, I will be there</label> <br>
                            <label for="no"><input type="checkbox" id="no"> Sorry, I can't come</label> <br>
                        </div>
                        <div class="form-group">
                            <label for="note">Note</label>
                            <textarea name="" id="note" cols="20" rows="5" class="form-control"></textarea>
                        </div>
                        <div class="form-group">
                            <input type="submit" class="btn btn-primary btn-lg" value="Send RSVP">
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="probootstrap-section probootstrap-section-colored-darker">
    <div class="container text-center">
        <div class="row">
            <div class="col-md-12 probootstrap-animate"><h2 class="mt0">Registry</h2></div>
        </div>
        <div class="row">
            <div class="col-md-12" id="registry-container"></div>
            {{#registryList}}
                <div class="thumbnail registry-card">
                   <img src="img/{{imgSrc}}" class="img-rounded img-responsive" />
                    <div class="caption">
                        <h5>{{title}}</h5>
                        <p>{{subText}}</p>
                        <p class="registry-cost">{{costF}}</p>
                        <div class="progress">
                            <div class="progress-bar" style="width:{{percentageF}};min-width:3em;">{{progressF}}</div>
                        </div>
                        <button class="btn btn-primary" type="button">Donate</button>
                    </div>
                </div>
            {{/registryList}}
        </div>
    </div>
</section>

